{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [ "config:base" ],
  "regexManagers": [ {
    "fileMatch": [ "^internal/src/main/kotlin/org/jsonschema2dataclass/internal/plugin/SpotlessPlugin.kt$" ],
    "matchStrings": [ "project,\\n\\s*styleCheckers,\\n\\s*\"(?<depName>[^\"]*?)\",\\n\\s*\"(?<currentValue>[^\"]*)\"" ],
    "datasourceTemplate": "gradle-version"
  } ],
  "packageRules": [ {
    "description": "Disable updating AGP compatibility dependencies. Allow changes in minor and patches",
    "matchPackageNames": [ "com.android.tools.build:gradle" ],
    "matchUpdateTypes": [ "major" ],
    "matchFiles": [ "demo/android-agp7/build.gradle.kts", "demo/android-agp7/settings.gradle.kts", "plugin-gradle/compat/agp7/build.gradle.kts", "plugin-gradle/compat/agp34/build.gradle.kts" ],
    "enabled": false
  } ]
}
